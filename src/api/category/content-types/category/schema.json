{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-z0-9_-]+$"
    },
    "post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post.post",
      "mappedBy": "category"
    }
  }
}
